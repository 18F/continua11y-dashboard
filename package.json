{
  "name": "continu-a11y",
  "version": "1.0.0",
  "description": "Riff on stvnrlly's continua11y, CI dashoard and tools",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha --recursive --slow 1500",
    "lint": "node_modules/.bin/jshint app/ test/",
    "start": "node index.js",
    "dev": "node_modules/.bin/nodemon -e js,mustache index.js",
    "dev_sass": "node_modules/.bin/nodemon -e scss -x \"node scripts/compile-scss\"",
    "test_coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/continua11y-due.git"
  },
  "keywords": [
    "accessibility",
    "CI",
    "dashboard"
  ],
  "author": "baccigalupi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/18F/continua11y-due/issues"
  },
  "homepage": "https://github.com/18F/continua11y-due#readme",
  "dependencies": {
    "async": "^2.1.2",
    "body-parser": "^1.15.2",
    "cfenv": "^1.0.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "knex": "^0.12.2",
    "lodash": "^4.16.4",
    "mustache": "^2.2.1",
    "node-sass": "^3.10.1",
    "pg": "^6.1.0",
    "uswds": "^0.13.1"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.4.0",
    "continua11y-acceptance": "^1.1.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.3",
    "mocha": "^3.1.0",
    "nodemon": "^1.11.0",
    "pa11y": "^4.0.1",
    "pa11y-ci": "^0.3.0",
    "zombie": "^4.2.1"
  }
}
